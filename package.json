    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "mongoose": "^5.12.0",
    "morgan": "^1.10.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "concurrently": "^6.0.0",
    "nodemon": "^2.0.4"
